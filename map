<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var repo = getRepoDetailsByName("Store Directory Banner");
            if (repo != undefined) {
                var banner = getImageURL(repo.images[0].photo_url);
                $(".inside_banner").css({backgroundImage: "url(" + banner + ")"});
            }
            
            show_content();
        }
        loadMallDataCached(renderAll); 
    });
</script>