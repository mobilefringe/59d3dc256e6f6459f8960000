<script>
    $(document).ready(function(e) {
        init(e);
        function renderAll (){
            renderPopup();
            
            var banners = getBanners();
            renderBanner('#banner_template','#home_banner', banners);
            
            var all_feature_items = getFeatureList();
            var three_items = all_feature_items.slice(0, 3);
            var single_item = all_feature_items.slice(3, 5);
            renderFeatureItems('#feature_container','#feature_template', three_items);
            renderFeatureItems('#single_feature_container','#single_feature_template', single_item);

            // RESTAURANTS //
            var restaurants = getStoresListByCategoryID(2538);
            renderStoreList('#home_dine_container','#home_dine_template', restaurants, "stores");
            
            // LOGO SCROLL //
            var stores = getStoresList();
            renderLogoScroll('#logo_container','#logo_template', stores);

            show_content();
            
            $('.flexslider').flexslider({
                animation: "slide",
            });
            
            var logo_options = {
                $AutoPlay: true,
                $AutoPlaySteps: 1,                                 
                $AutoPlayInterval: 0,                            
                $PauseOnHover: 4,                               
                $ArrowKeyNavigation: true,   			            
                $SlideEasing: $JssorEasing$.$EaseLinear,          
                $SlideDuration: 3000,                                
                $MinDragOffsetToSlide: 20,                          
                $SlideWidth: 160,                                                                  
                $SlideSpacing: 50, 					                
                $DisplayPieces: 7,                                 
                $ParkingPosition: 0,                              
                $UISearchMode: 1,                                  
                $PlayOrientation: 1,                                
                $DragOrientation: 1                               
            };

            var logo_slider = new $JssorSlider$("slider_container", logo_options);
        }
        loadMallDataCached(renderAll); 
    })
</script>